<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/transparentLayer"
    android:background="#d8424242">

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_weight="2">

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="fill_parent"
            android:layout_weight="13">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="Bucuresti"
                android:id="@+id/textView3"
                android:layout_gravity="center"
                android:textColor="#ffffff" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:text="1 Iulie - 30 Septembrie 2015"
                android:id="@+id/textView5"
                android:layout_gravity="center"
                android:textColor="#ffffff" />

        </LinearLayout>

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="2">

            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="fill_parent"
                android:layout_weight="1">

                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="fill_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:paddingRight="10dp">

                    <LinearLayout
                        android:orientation="vertical"
                        android:layout_width="fill_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:paddingRight="5dp">

                        <TextView
                            android:layout_width="fill_parent"
                            android:layout_height="fill_parent"
                            android:textAppearance="?android:attr/textAppearanceLarge"
                            android:text=""
                            android:id="@+id/textViewNrBuyers"
                            android:layout_weight="1"
                            android:textColor="#f9a792"
                            android:textAlignment="textEnd"
                            android:gravity="bottom"
                            android:textStyle="bold" />
                        <TextView
                            android:layout_width="fill_parent"
                            android:layout_height="fill_parent"
                            android:textAppearance="?android:attr/textAppearanceSmall"
                            android:text="Autoritati\ncontractante"
                            android:id="@+id/textView6"
                            android:layout_weight="1"
                            android:textColor="#f9a792"
                            android:gravity="top|right"/>

                    </LinearLayout>

                    <ImageView
                        android:layout_width="fill_parent"
                        android:layout_height="match_parent"
                        android:id="@+id/imageView2"
                        android:src="@drawable/library"
                        android:layout_weight="3" />

                </LinearLayout>

                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="fill_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:paddingLeft="10dp">

                    <ImageView
                        android:layout_width="fill_parent"
                        android:layout_height="match_parent"
                        android:id="@+id/imageView3"
                        android:src="@drawable/company"
                        android:layout_weight="3" />

                    <LinearLayout
                        android:orientation="vertical"
                        android:layout_width="fill_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:paddingRight="5dp">
                        <TextView
                            android:layout_width="fill_parent"
                            android:layout_height="fill_parent"
                            android:textAppearance="?android:attr/textAppearanceLarge"
                            android:text=""
                            android:id="@+id/textViewNrCompanies"
                            android:layout_weight="1"
                            android:textColor="#c57ffa"
                            android:textAlignment="textStart"
                            android:textStyle="bold"
                            android:gravity="bottom" />

                        <TextView
                            android:layout_width="fill_parent"
                            android:layout_height="fill_parent"
                            android:textAppearance="?android:attr/textAppearanceSmall"
                            android:text="Companii"
                            android:id="@+id/textView19"
                            android:layout_weight="1"
                            android:textColor="#c57ffa"
                            android:gravity="top|left" />
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="fill_parent"
                android:layout_weight="1">

                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="fill_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:paddingRight="5dp">

                    <LinearLayout
                        android:orientation="vertical"
                        android:layout_width="fill_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:paddingRight="10dp">

                        <TextView
                            android:layout_width="fill_parent"
                            android:layout_height="fill_parent"
                            android:textAppearance="?android:attr/textAppearanceLarge"
                            android:text=""
                            android:id="@+id/textViewNrContracts"
                            android:layout_weight="1"
                            android:textColor="#ffdb44"
                            android:textAlignment="textEnd"
                            android:gravity="bottom"
                            android:textStyle="bold" />
                        <TextView
                            android:layout_width="fill_parent"
                            android:layout_height="fill_parent"
                            android:textAppearance="?android:attr/textAppearanceSmall"
                            android:text="Contracte"
                            android:id="@+id/textView61"
                            android:layout_weight="1"
                            android:textColor="#ffdb44"
                            android:gravity="top|right"/>
                    </LinearLayout>

                    <ImageView
                        android:layout_width="fill_parent"
                        android:layout_height="match_parent"
                        android:id="@+id/imageView21"
                        android:src="@drawable/contract"
                        android:layout_weight="3" />
                </LinearLayout>

                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="fill_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:paddingLeft="10dp">

                    <ImageView
                        android:layout_width="fill_parent"
                        android:layout_height="match_parent"
                        android:id="@+id/imageView31"
                        android:src="@drawable/eurosymbol"
                        android:layout_weight="3" />

                    <LinearLayout
                        android:orientation="vertical"
                        android:layout_width="fill_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:paddingRight="5dp">

                        <TextView
                            android:layout_width="fill_parent"
                            android:layout_height="fill_parent"
                            android:textAppearance="?android:attr/textAppearanceLarge"
                            android:text=""
                            android:id="@+id/textViewValueContracts"
                            android:layout_weight="1"
                            android:textColor="#a8e87b"
                            android:textAlignment="textStart"
                            android:textStyle="bold"
                            android:gravity="bottom" />
                        <TextView
                            android:layout_width="fill_parent"
                            android:layout_height="fill_parent"
                            android:textAppearance="?android:attr/textAppearanceSmall"
                            android:text="Valoare contracte\n(mil EURO)"
                            android:id="@+id/textView191"
                            android:layout_weight="1"
                            android:textColor="#a8e87b"
                            android:gravity="top|left" />
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="fill_parent"
                android:layout_weight="1">

                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent"
                    android:layout_weight="1">

                    <TextView
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:textAppearance="?android:attr/textAppearanceMedium"
                        android:id="@+id/textView22"
                        android:layout_weight="1"
                        android:text=" " />

                    <TextView
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:textAppearance="?android:attr/textAppearanceSmall"
                        android:text=""
                        android:id="@+id/textView25"
                        android:layout_weight="1" />

                    <ImageView
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:id="@+id/imageView4"
                        android:src="@drawable/question"
                        android:layout_weight="1" />

                    <TextView
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:textAppearance="?android:attr/textAppearanceLarge"
                        android:text=""
                        android:id="@+id/textViewNrJustifies"
                        android:layout_weight="0.95"
                        android:textColor="#77cbd4"
                        android:singleLine="false"
                        android:gravity="bottom|left"
                        android:textStyle="bold" />

                    <TextView
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:textAppearance="?android:attr/textAppearanceSmall"
                        android:text=""
                        android:id="@+id/textView23"
                        android:layout_weight="1" />

                    <TextView
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:textAppearance="?android:attr/textAppearanceSmall"
                        android:text=""
                        android:id="@+id/textView24"
                        android:layout_weight="1" />

                </LinearLayout>

                <TextView
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent"
                    android:textAppearance="?android:attr/textAppearanceSmall"
                    android:text="Cereri de justificari"
                    android:id="@+id/textView21"
                    android:layout_weight="1"
                    android:textColor="#77cbd4"
                    android:layout_gravity="center"
                    android:gravity="center_horizontal|top" />
            </LinearLayout>
        </LinearLayout>


    </LinearLayout>

    <FrameLayout
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"

        android:layout_weight="4"
        android:background="#00ffffff"
        android:paddingBottom="10dp">

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:paddingLeft="20dp"
            android:paddingTop="5dp"
            android:weightSum="1">

            <TextView
                android:layout_width="fill_parent"
                android:layout_height="41dp"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:id="@+id/textView14"
                android:textAlignment="textStart"
                android:textColor="#ffffff"
                android:gravity="start"
                android:maxHeight="100dp"
                android:layout_weight="0.38" />

            <TextView
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:text="@string/info1"
                android:id="@+id/textView4"
                android:textAlignment="center"
                android:textColor="#ffffff"
                android:gravity="start"
                android:layout_gravity="center"
                android:layout_weight="0.23" />




            <TextView
                android:layout_width="fill_parent"
                android:layout_height="50dp"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:text="@string/info4"
                android:id="@+id/textView12"
                android:textAlignment="center"
                android:textColor="#ffffff"
                android:gravity="start" />



            <TextView
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:id="@+id/textView13"
                android:textAlignment="textStart"
                android:textColor="#ffffff"
                android:gravity="start" />

        </LinearLayout>

        <FrameLayout
            android:layout_width="90dp"
            android:layout_height="50dp"
            android:layout_gravity="center_horizontal|top">

            <Button
                android:layout_width="80dp"
                android:layout_height="40dp"
                android:text="Ok"
                android:id="@+id/okButton"
                android:gravity="center"
                android:background="@drawable/rounded_ok_button"
                android:textColor="#ffffff"
                android:layout_gravity="center" />
        </FrameLayout>

    </FrameLayout>

</LinearLayout>


