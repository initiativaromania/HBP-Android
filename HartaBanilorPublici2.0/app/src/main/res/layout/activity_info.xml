<merge xmlns:android="http://schemas.android.com/apk/res/android">

    <ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
        android:orientation="vertical" android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#ffffff">

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="fill_parent"
                android:layout_height="44dp"
                android:background="#ffffff">

                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:layout_gravity="center">

                    <ImageButton
                        android:layout_width="41dp"
                        android:layout_height="35dp"
                        android:id="@+id/imageViewContract"
                        android:src="@drawable/fb_profile_photo"
                        android:background="#ffffff"
                        android:scaleType="fitCenter"
                        android:layout_gravity="center" />

                    <TextView
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:textAppearance="?android:attr/textAppearanceLarge"
                        android:text="Informatii"
                        android:id="@+id/textViewInfo"
                        android:textColor="#77cbd4"
                        android:textColorHighlight="#953232"
                        android:textSize="25dp"
                        android:visibility="visible"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:background="#ffffff"
                        android:textStyle="bold" />

                </LinearLayout>

                <ImageButton
                    android:layout_width="45dp"
                    android:layout_height="40dp"
                    android:id="@+id/imageButton"
                    android:src="@drawable/info"
                    android:background="#ffffff"
                    android:scaleType="fitCenter"
                    android:layout_gravity="center" />

            </LinearLayout>

            <RelativeLayout
                android:layout_width="fill_parent"
                android:layout_height="300dp"
                android:paddingLeft="10dp"
                android:paddingRight="10dp">

                <ImageView
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent"
                    android:id="@+id/imageView5"
                    android:src="@drawable/google_map"
                    android:scaleType="centerCrop" />

                <LinearLayout
                android:orientation="vertical"
                android:layout_width="fill_parent"
                android:layout_height="300dp"
                android:background="#b9494949">


                <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="match_parent"
                    android:layout_height="fill_parent"
                    android:layout_weight="13">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textAppearance="?android:attr/textAppearanceMedium"
                        android:text="Bucuresti"
                        android:id="@+id/textView3"
                        android:layout_gravity="center"
                        android:textColor="#ffffff"/>

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textAppearance="?android:attr/textAppearanceSmall"
                        android:text="1 Iulie - 30 Septembrie 2015"
                        android:id="@+id/textView5"
                        android:layout_gravity="center"
                        android:textColor="#ffffff"/>

                </LinearLayout>

                <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="2">

                    <LinearLayout
                        android:orientation="horizontal"
                        android:layout_width="match_parent"
                        android:layout_height="fill_parent"
                        android:layout_weight="1">

                        <LinearLayout
                            android:orientation="horizontal"
                            android:layout_width="fill_parent"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:paddingRight="10dp">

                            <LinearLayout
                                android:orientation="vertical"
                                android:layout_width="fill_parent"
                                android:layout_height="match_parent"
                                android:layout_weight="1"
                                android:paddingRight="5dp">

                                <TextView
                                    android:layout_width="fill_parent"
                                    android:layout_height="fill_parent"
                                    android:textAppearance="?android:attr/textAppearanceLarge"
                                    android:text=""
                                    android:id="@+id/textViewNrBuyers"
                                    android:layout_weight="1"
                                    android:textColor="#f9a792"
                                    android:textAlignment="textEnd"
                                    android:gravity="bottom"
                                    android:textStyle="bold" />
                                <TextView
                                    android:layout_width="fill_parent"
                                    android:layout_height="fill_parent"
                                    android:textAppearance="?android:attr/textAppearanceSmall"
                                    android:text="Autoritati\ncontractante"
                                    android:id="@+id/textView6"
                                    android:layout_weight="1"
                                    android:textColor="#f9a792"
                                    android:gravity="top|right"/>

                            </LinearLayout>

                            <ImageView
                                android:layout_width="fill_parent"
                                android:layout_height="match_parent"
                                android:id="@+id/imageView2"
                                android:src="@drawable/library"
                                android:layout_weight="3" />

                        </LinearLayout>

                        <LinearLayout
                            android:orientation="horizontal"
                            android:layout_width="fill_parent"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:paddingLeft="10dp">

                            <ImageView
                                android:layout_width="fill_parent"
                                android:layout_height="match_parent"
                                android:id="@+id/imageView3"
                                android:src="@drawable/company"
                                android:layout_weight="3" />

                            <LinearLayout
                                android:orientation="vertical"
                                android:layout_width="fill_parent"
                                android:layout_height="match_parent"
                                android:layout_weight="1"
                                android:paddingRight="5dp">
                                    <TextView
                                        android:layout_width="fill_parent"
                                        android:layout_height="fill_parent"
                                        android:textAppearance="?android:attr/textAppearanceLarge"
                                        android:text=""
                                        android:id="@+id/textViewNrCompanies"
                                        android:layout_weight="1"
                                        android:textColor="#c57ffa"
                                        android:textAlignment="textStart"
                                        android:textStyle="bold"
                                        android:gravity="bottom" />

                                    <TextView
                                        android:layout_width="fill_parent"
                                        android:layout_height="fill_parent"
                                        android:textAppearance="?android:attr/textAppearanceSmall"
                                        android:text="Companii"
                                        android:id="@+id/textView19"
                                        android:layout_weight="1"
                                        android:textColor="#c57ffa"
                                        android:gravity="top|left" />
                            </LinearLayout>
                        </LinearLayout>
                    </LinearLayout>

                    <LinearLayout
                        android:orientation="horizontal"
                        android:layout_width="match_parent"
                        android:layout_height="fill_parent"
                        android:layout_weight="1">

                        <LinearLayout
                            android:orientation="horizontal"
                            android:layout_width="fill_parent"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:paddingRight="5dp">

                            <LinearLayout
                                android:orientation="vertical"
                                android:layout_width="fill_parent"
                                android:layout_height="match_parent"
                                android:layout_weight="1"
                                android:paddingRight="10dp">

                                <TextView
                                    android:layout_width="fill_parent"
                                    android:layout_height="fill_parent"
                                    android:textAppearance="?android:attr/textAppearanceLarge"
                                    android:text=""
                                    android:id="@+id/textViewNrContracts"
                                    android:layout_weight="1"
                                    android:textColor="#ffdb44"
                                    android:textAlignment="textEnd"
                                    android:gravity="bottom"
                                    android:textStyle="bold" />
                                <TextView
                                        android:layout_width="fill_parent"
                                        android:layout_height="fill_parent"
                                        android:textAppearance="?android:attr/textAppearanceSmall"
                                        android:text="Contracte"
                                        android:id="@+id/textView61"
                                        android:layout_weight="1"
                                        android:textColor="#ffdb44"
                                    android:gravity="top|right"/>
                            </LinearLayout>

                            <ImageView
                                android:layout_width="fill_parent"
                                android:layout_height="match_parent"
                                android:id="@+id/imageView21"
                                android:src="@drawable/contract"
                                android:layout_weight="3" />
                        </LinearLayout>

                        <LinearLayout
                            android:orientation="horizontal"
                            android:layout_width="fill_parent"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:paddingLeft="10dp">

                            <ImageView
                                android:layout_width="fill_parent"
                                android:layout_height="match_parent"
                                android:id="@+id/imageView31"
                                android:src="@drawable/eurosymbol"
                                android:layout_weight="3" />

                            <LinearLayout
                                android:orientation="vertical"
                                android:layout_width="fill_parent"
                                android:layout_height="match_parent"
                                android:layout_weight="1"
                                android:paddingRight="5dp">

                                    <TextView
                                        android:layout_width="fill_parent"
                                        android:layout_height="fill_parent"
                                        android:textAppearance="?android:attr/textAppearanceLarge"
                                        android:text=""
                                        android:id="@+id/textViewValueContracts"
                                        android:layout_weight="1"
                                        android:textColor="#a8e87b"
                                        android:textAlignment="textStart"
                                        android:textStyle="bold"
                                        android:gravity="bottom" />
                                    <TextView
                                        android:layout_width="fill_parent"
                                        android:layout_height="fill_parent"
                                        android:textAppearance="?android:attr/textAppearanceSmall"
                                        android:text="Valoare contracte\n(mil EURO)"
                                        android:id="@+id/textView191"
                                        android:layout_weight="1"
                                        android:textColor="#a8e87b"
                                        android:gravity="top|left" />
                            </LinearLayout>
                        </LinearLayout>
                    </LinearLayout>

                    <LinearLayout
                        android:orientation="vertical"
                        android:layout_width="match_parent"
                        android:layout_height="fill_parent"
                        android:layout_weight="1">

                        <LinearLayout
                            android:orientation="horizontal"
                            android:layout_width="fill_parent"
                            android:layout_height="fill_parent"
                            android:layout_weight="1">

                            <TextView
                                android:layout_width="fill_parent"
                                android:layout_height="fill_parent"
                                android:textAppearance="?android:attr/textAppearanceMedium"
                                android:id="@+id/textView22"
                                android:layout_weight="1"
                                android:text=" " />

                            <TextView
                                android:layout_width="fill_parent"
                                android:layout_height="fill_parent"
                                android:textAppearance="?android:attr/textAppearanceSmall"
                                android:text=""
                                android:id="@+id/textView25"
                                android:layout_weight="1" />

                            <ImageView
                                android:layout_width="fill_parent"
                                android:layout_height="fill_parent"
                                android:id="@+id/imageView4"
                                android:src="@drawable/question"
                                android:layout_weight="1" />

                            <TextView
                                android:layout_width="fill_parent"
                                android:layout_height="fill_parent"
                                android:textAppearance="?android:attr/textAppearanceLarge"
                                android:text=""
                                android:id="@+id/textViewNrJustifies"
                                android:layout_weight="0.95"
                                android:textColor="#77cbd4"
                                android:singleLine="false"
                                android:gravity="bottom|left"
                                android:textStyle="bold" />

                            <TextView
                                android:layout_width="fill_parent"
                                android:layout_height="fill_parent"
                                android:textAppearance="?android:attr/textAppearanceSmall"
                                android:text=""
                                android:id="@+id/textView23"
                                android:layout_weight="1" />

                            <TextView
                                android:layout_width="fill_parent"
                                android:layout_height="fill_parent"
                                android:textAppearance="?android:attr/textAppearanceSmall"
                                android:text=""
                                android:id="@+id/textView24"
                                android:layout_weight="1" />

                        </LinearLayout>

                        <TextView
                            android:layout_width="fill_parent"
                            android:layout_height="fill_parent"
                            android:textAppearance="?android:attr/textAppearanceSmall"
                            android:text="Cereri de justificari"
                            android:id="@+id/textView21"
                            android:layout_weight="1"
                            android:textColor="#77cbd4"
                            android:layout_gravity="center"
                            android:gravity="center_horizontal|top" />
                    </LinearLayout>
                </LinearLayout>


            </LinearLayout>

            </RelativeLayout>


            <LinearLayout
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:paddingLeft="10dp"
                android:paddingRight="10dp">

                <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="20dp">

                <View
                    android:layout_width="fill_parent"
                    android:layout_height="1dip"
                    android:id="@+id/view1011"
                    android:background="#9d9d9d"
                    android:layout_gravity="center_vertical"
                    android:layout_weight="6" />

                <TextView
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:text="Descriere"
                    android:id="@+id/textView111"
                    android:layout_gravity="center_vertical|top"
                    android:layout_weight="4"
                    android:textAlignment="center"
                    android:gravity="top" />

                <View
                    android:layout_width="fill_parent"
                    android:layout_height="1dip"
                    android:id="@+id/view1021"
                    android:background="#9d9d9d"
                    android:layout_gravity="center_vertical"
                    android:layout_weight="6" />

            </LinearLayout>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:textAppearance="?android:attr/textAppearanceSmall"
                    android:text="@string/descriere1"
                    android:id="@+id/textView10" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:textAppearance="?android:attr/textAppearanceSmall"
                    android:text="@string/descriere2"
                    android:id="@+id/textView101" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:textAppearance="?android:attr/textAppearanceSmall"
                    android:text="@string/descriere3"
                    android:id="@+id/textView102" />

                <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="20dp">

                <View
                    android:layout_width="fill_parent"
                    android:layout_height="1dip"
                    android:id="@+id/view1012"
                    android:background="#9d9d9d"
                    android:layout_gravity="center_vertical"
                    android:layout_weight="6" />

                <TextView
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:text="Disclaimer"
                    android:id="@+id/textView113"
                    android:layout_gravity="center_vertical|top"
                    android:layout_weight="4"
                    android:textAlignment="center"
                    android:gravity="top" />

                <View
                    android:layout_width="fill_parent"
                    android:layout_height="1dip"
                    android:id="@+id/view104"
                    android:background="#9d9d9d"
                    android:layout_gravity="center_vertical"
                    android:layout_weight="6" />

            </LinearLayout>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:textAppearance="?android:attr/textAppearanceSmall"
                    android:text="@string/disclaimer1"
                    android:id="@+id/textView15" />

                <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="20dp">

                <View
                    android:layout_width="fill_parent"
                    android:layout_height="1dip"
                    android:id="@+id/view1014"
                    android:background="#9d9d9d"
                    android:layout_gravity="center_vertical"
                    android:layout_weight="6" />

                <TextView
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:text="Versiune"
                    android:id="@+id/textView115"
                    android:layout_gravity="center_vertical|top"
                    android:layout_weight="4"
                    android:textAlignment="center"
                    android:gravity="top" />

                <View
                    android:layout_width="fill_parent"
                    android:layout_height="1dip"
                    android:id="@+id/view105"
                    android:background="#9d9d9d"
                    android:layout_gravity="center_vertical"
                    android:layout_weight="6" />

            </LinearLayout>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:textAppearance="?android:attr/textAppearanceSmall"
                    android:text="@string/versiune"
                    android:id="@+id/textView151"
                    android:layout_gravity="center" />

                <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="20dp">

                <View
                    android:layout_width="fill_parent"
                    android:layout_height="1dip"
                    android:id="@+id/view101"
                    android:background="#9d9d9d"
                    android:layout_gravity="center_vertical"
                    android:layout_weight="6" />

                <TextView
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:text="Initiativa Romania"
                    android:id="@+id/textView11"
                    android:layout_gravity="center_vertical|top"
                    android:layout_weight="4"
                    android:textAlignment="center"
                    android:gravity="top" />

                <View
                    android:layout_width="fill_parent"
                    android:layout_height="1dip"
                    android:id="@+id/view102"
                    android:background="#9d9d9d"
                    android:layout_gravity="center_vertical"
                    android:layout_weight="6" />

            </LinearLayout>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:autoLink="web"
                    android:textAppearance="?android:attr/textAppearanceSmall"
                    android:text="@string/initiativaromania"
                    android:id="@+id/textView152" />
            </LinearLayout>


        </LinearLayout>

    </ScrollView>

</merge>